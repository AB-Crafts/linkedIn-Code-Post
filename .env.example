# Supabase Configuration
# Get these values from your Supabase project settings > API

SUPABASE_URL=your_supabase_url_here
SUPABASE_ANON_KEY=your_anon_key_here

# Web3Forms Configuration (already in use)
WEB3FORMS_ACCESS_KEY=your_access_key_here

# Resend Configuration (for sending emails to waitlist)
# Get your API key from https://resend.com/api-keys
RESEND_API_KEY=re_your_api_key_heres
FROM_EMAIL=noreply@pushtopost.dev
FROM_NAME=PushToPost Team

# Supabase Service Role Key (for server-side operations)
# Get this from Supabase Dashboard > Settings > API > service_role key
# ⚠️ NEVER expose this key in client-side code!
SUPABASE_SERVICE_KEY=your_service_role_key_here
